with open('/Users/f/Desktop/data_example/data_example2.csv','r') as _csvfile:
    csv_reader = csv.reader(_csvfile, delimiter = '/')
    with open('/Users/f/Desktop/data_example/data_example3.csv','w') as _csvfile:
        csv_writer = csv.writer(_csvfile)
        for line in csv_reader:
            csv_writer.writerow(line)
